{
  "name": "feast",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:shared": "turbo run build --filter=@feast/shared",
    "build:api": "turbo run build --filter=@feast/api",
    "build:web": "turbo run build --filter=@feast/web",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prisma:migrate": "cd apps/api && npx prisma migrate dev",
    "prisma:reset": "cd apps/api && npx prisma migrate reset",
    "start:api": "cd apps/api && npm run start",
    "start:web": "cd apps/web && npm run start"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "tsc-alias": "^1.8.16",
    "zod": "^3.25.20"
  }
}
